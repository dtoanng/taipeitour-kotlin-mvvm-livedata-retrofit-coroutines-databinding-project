<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="ScrollEdgeEffect" parent="android:ThemeOverlay.Material">
        <item name="android:colorEdgeEffect">@color/COLOR_3695DD</item>
    </style>

    <style name="subHeader">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textStyle">normal|bold</item>
        <item name="android:textColor">@color/SUB_HEADER</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">26dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="backButton">
        <item name="android:src">@drawable/arrow_back</item>
    </style>

    <style name="ShapeAppearanceOverlayExtended" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">0dp</item>
        <item name="cornerSizeTopLeft">50%</item>
    </style>
</resources>